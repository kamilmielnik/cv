{
  "name": "cv",
  "version": "1.4.0",
  "description": "Kamil Mielnik's Curriculum Vitae",
  "main": "src/index.js",
  "engines": {
    "node": ">=7.9.0"
  },
  "scripts": {
    "build": "better-npm-run build",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "prettier:js": "npm run lint -- --no-eslintrc -c .eslintrc.prettier-only.js",
    "prettier:js:fix": "npm run prettier:js -- --fix",
    "prettier:scss": "prettier --list-different './**/*.scss'",
    "prettier:scss:fix": "npm run prettier:scss -- --write --no-list-different",
    "prettier": "npm run prettier:js && npm run prettier:scss",
    "prettier:fix": "npm run prettier:js:fix && npm run prettier:scss:fix",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "build": {
      "command": "react-scripts build",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "src/"
      }
    },
    "start": {
      "command": "react-scripts start",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "src/"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/cv.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "http://kamilmielnik.com/"
  },
  "license": "CC BY-NC-ND 4.0",
  "bugs": {
    "url": "https://github.com/kamilmielnik/cv/issues"
  },
  "homepage": "https://github.com/kamilmielnik/cv#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "better-npm-run": "^0.1.1",
    "eslint": "5.12.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-scripts": "^2.1.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
