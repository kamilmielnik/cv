{
  "name": "cv",
  "version": "1.6.3",
  "description": "Kamil Mielnik's Curriculum Vitae",
  "engines": {
    "node": ">=12.4.0"
  },
  "scripts": {
    "build": "better-npm-run build && npm run build:inline-css && npm run build:pdf && npm run build:sitemap",
    "build:inline-css": "node scripts/inlineCss.js",
    "build:pdf": "node scripts/createPdf.js",
    "build:sitemap": "node scripts/createSitemap.js",
    "clean": "rimraf build",
    "deploy": "node scripts/deploy.js",
    "lint": "eslint src/ scripts/ --ext '.js,.jsx,.ts,.tsx'",
    "lint:fix": "eslint src/ scripts/ --ext '.js,.jsx,.ts,.tsx' --fix",
    "prettier": "npm run prettier:js && npm run prettier:scss",
    "prettier:fix": "npm run prettier:js:fix && npm run prettier:scss:fix",
    "prettier:js": "prettier --list-different './src/**/*.{js,jsx,ts,tsx}'",
    "prettier:js:fix": "npm run prettier:js -- --write --no-list-different",
    "prettier:scss": "prettier --list-different './src/**/*.scss'",
    "prettier:scss:fix": "npm run prettier:scss -- --write --no-list-different",
    "publish": "npm run clean && npm run build && npm run deploy",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "build": {
      "command": "react-scripts build",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "src/"
      }
    },
    "start": {
      "command": "react-scripts start",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "src/"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/cv.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "https://kamilmielnik.com/"
  },
  "license": "CC BY-NC-ND 4.0",
  "bugs": {
    "url": "https://github.com/kamilmielnik/cv/issues"
  },
  "homepage": "https://cv.kamilmielnik.com",
  "dependencies": {
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-hot-loader": "^4.1.1",
    "better-npm-run": "^0.1.1",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "http-server": "^0.12.1",
    "node-sass": "^4.13.1",
    "node-ssh": "^8.0.0",
    "prettier": "^2.0.4",
    "puppeteer": "^2.1.1",
    "react-scripts": "^3.4.1",
    "rimraf": "^3.0.2",
    "sitemap": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
