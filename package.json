{
  "name": "cv",
  "version": "1.9.2",
  "description": "Kamil Mielnik's Curriculum Vitae",
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/cv.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil@kamilmielnik.com",
    "url": "https://kamilmielnik.com"
  },
  "license": "CC BY-NC-ND 4.0",
  "bugs": {
    "url": "https://github.com/kamilmielnik/cv/issues"
  },
  "homepage": "https://cv.kamilmielnik.com",
  "scripts": {
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "dev": "NEXT_TELEMETRY_DISABLED=1 next dev",
    "lint": "eslint src/ --ext '.js,.jsx,.ts,.tsx'",
    "lint:fix": "eslint src/ --ext '.js,.jsx,.ts,.tsx' --fix",
    "prettier": "npm run prettier:js && npm run prettier:scss",
    "prettier:fix": "npm run prettier:js:fix && npm run prettier:scss:fix",
    "prettier:js": "prettier --list-different './src/**/*.{js,jsx,ts,tsx}'",
    "prettier:js:fix": "npm run prettier:js -- --write --no-list-different",
    "prettier:scss": "prettier --list-different './src/**/*.scss'",
    "prettier:scss:fix": "npm run prettier:scss -- --write --no-list-different",
    "start": "NEXT_TELEMETRY_DISABLED=1 next start",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "fingerprintjs2": "^2.1.2",
    "lowdb": "^3.0.0",
    "next": "^13.2.4",
    "normalize.css": "^8.0.1",
    "puppeteer": "^19.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/fingerprintjs2": "^2.0.0",
    "@types/jest": "^29.5.0",
    "@types/lowdb": "^1.0.11",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "svg-sprite-loader": "^6.0.11",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3",
    "webpack": "^5.77.0"
  }
}
