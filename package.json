{
  "name": "cv",
  "version": "1.2.3",
  "description": "Kamil Mielnik's Curriculum Vitae",
  "main": "src/index.js",
  "engines": {
    "node": ">=7.9.0"
  },
  "scripts": {
    "build": "better-npm-run build",
    "lint": "better-npm-run lint",
    "lint:fix": "better-npm-run lint:fix",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack-wizard build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint": {
      "command": "eslint src/ --ext .js"
    },
    "lint:fix": {
      "command": "eslint src/ --ext .js --fix"
    },
    "start": {
      "command": "webpack-wizard start",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:prod": {
      "command": "http-server dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/cv.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "http://kamilmielnik.com/"
  },
  "license": "CC BY-NC-ND 4.0",
  "bugs": {
    "url": "https://github.com/kamilmielnik/cv/issues"
  },
  "homepage": "https://github.com/kamilmielnik/cv#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "sidebar": "git://github.com/kamilmielnik/sidebar.git"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "better-npm-run": "0.1.0",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "http-server": "^0.11.1",
    "react-hot-loader": "^3.1.3",
    "webpack-wizard": "0.0.9"
  }
}
