  "scripts": {
    "dev": "NEXT_TELEMETRY_DISABLED=1 next dev",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "lint": "eslint src/ --ext '.js,.jsx,.ts,.tsx'",
    "lint:fix": "eslint src/ --ext '.js,.jsx,.ts,.tsx' --fix",
    "prettier": "npm run prettier:js && npm run prettier:scss",
    "prettier:fix": "npm run prettier:js:fix && npm run prettier:scss:fix",
    "prettier:js": "prettier --list-different './src/**/*.{js,jsx,ts,tsx}'",
    "prettier:js:fix": "npm run prettier:js -- --write --no-list-different",
    "prettier:scss": "prettier --list-different './src/**/*.scss'",
    "prettier:scss:fix": "npm run prettier:scss -- --write --no-list-different",
    "start": "NEXT_TELEMETRY_DISABLED=1 next start",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@types/lowdb": "^1.0.9",
    "classnames": "^2.2.6",
    "fingerprintjs2": "^2.1.0",
    "lowdb": "^1.0.0",
    "next": "9.4.4",
    "normalize.css": "^8.0.1",
    "puppeteer": "^4.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/fingerprintjs2": "^2.0.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/puppeteer": "^3.0.0",
    "@types/react": "^16.9.41",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.9",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5"
  }
