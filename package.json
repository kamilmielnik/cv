{
  "name": "cv",
  "version": "1.1.0",
  "description": "Kamil Mielnik's Curriculum Vitae",
  "main": "src/index.js",
  "engines": {
    "node": ">=7.9.0"
  },
  "scripts": {
    "build": "better-npm-run build",
    "lint": "better-npm-run lint",
    "lint:fix": "better-npm-run lint:fix",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint": {
      "command": "eslint src/ --ext .js"
    },
    "lint:fix": {
      "command": "eslint src/ --ext .js --fix"
    },
    "start": {
      "command": "node dev-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:prod": {
      "command": "http-server dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/cv.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "http://kamilmielnik.pl/"
  },
  "license": "CC BY-NC-ND 4.0",
  "bugs": {
    "url": "https://github.com/kamilmielnik/cv/issues"
  },
  "homepage": "https://github.com/kamilmielnik/cv#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sidebar": "git://github.com/kamilmielnik/sidebar.git"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "better-npm-run": "0.1.0",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "image-webpack-loader": "^3.4.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "react-hot-loader": "^3.1.1",
    "resolve-url-loader": "^2.1.1",
    "sass-loader": "^6.0.6",
    "style-ext-html-webpack-plugin": "^3.4.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  }
}
